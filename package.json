{
  "name": "csgofrontend",
  "version": "1.0.0",
  "description": "CSGO Frontend",
  "main": "index.js",
  "scripts": {
    "esbuild:base": "esbuild src/js/index.js --bundle --loader:.js=jsx --format=cjs --outfile=public/dist/bundle.js",
    "build:esbuild": "npm run esbuild:base -- --minify",
    "watch:esbuild": "npm run esbuild:base -- --watch",
    "config": "ncp config/settings-prod.js src/js/settings.js --stopOnErr",
    "config:dev": "ncp config/settings-dev.js src/js/settings.js --stopOnErr",
    "config:dev:watch": "nodemon --exec \"ncp config/settings-dev.js src/js/settings.js --stopOnErr\" --watch config/settings-dev.js",
    "serve": "serve public/ -s",
    "open": "opener https://127.0.0.1:3000/",
    "build": "npm run config && npm run build:esbuild",
    "build:dev": "npm run config:dev && npm run build:esbuild",
    "watch": "concurrently --kill-others-on-fail npm:config:dev:watch npm:watch:esbuild npm:serve npm:open",
    "cypress": "wait-on http://localhost:3000 && cypress run",
    "test": "concurrently --kill-others npm:serve npm:cypress"
  },
  "homepage": "./",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.1",
    "cypress": "^13.6.4",
    "esbuild": "0.20.0",
    "opener": "^1.5.2",
    "postcss": "^8.4.34",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@mantine/carousel": "^7.5.1",
    "@mantine/charts": "^7.5.1",
    "@mantine/code-highlight": "^7.5.1",
    "@mantine/core": "^7.5.1",
    "@mantine/dates": "^7.5.1",
    "@mantine/dropzone": "^7.5.1",
    "@mantine/form": "^7.5.1",
    "@mantine/hooks": "^7.5.1",
    "@mantine/modals": "^7.5.1",
    "@mantine/notifications": "^7.5.1",
    "@mantine/nprogress": "^7.5.1",
    "@mantine/spotlight": "^7.5.1",
    "@mantine/tiptap": "^7.5.1",
    "@tabler/icons-react": "^2.47.0",
    "@tiptap/extension-link": "^2.2.1",
    "@tiptap/react": "^2.2.1",
    "@tiptap/starter-kit": "^2.2.1",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^7.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.11.0"
  }
}
